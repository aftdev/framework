{
  "name": "aftdev/filesystem-manager",
  "description": "Filesystem Manager based on flysystem (v2)",
  "license": "BSD-3-Clause",
  "keywords": [
    "flysystem"
  ],
  "require": {
    "php": "^8.1",
    "league/flysystem": "^3.0.0",
    "aftdev/service-manager": "^1.1",
    "illuminate/support": "^9.0"
  },
  "require-dev": {
    "league/flysystem-memory": "^3.0",
    "league/flysystem-aws-s3-v3": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "AftDev\\Filesystem\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "AftDevTest\\Filesystem\\": "tests/"
    }
  },
  "extra": {
    "laminas": {
      "config-provider": "AftDev\\Filesystem\\ConfigProvider"
    }
  }
}
